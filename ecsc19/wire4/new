    {
      id: "support",
      itemdata: [
            { text: "Network configurations", url: "/pta_tools/net_configuration" },
            { text: "Network tools", url: "/pta_tools/net_tools" },
        { text: "Send invitation", url: "/invitations/new" },
        { text: "Change password", url: "/passwords/change" },
        { text: "Last Logins", url: "/lastlogins" },
