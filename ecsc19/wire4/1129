    jQuery(document).ready(function() {
            // Disabled log for production environment
            console.warn = function() {};

        var ptf_instrumentation = new PtfInstrumentation();
        var probesTimeline = new ProbesTimeline(context, '#probes-timeline');
        var displayedVulns = {};

        // probes timeline chart time domain changed
        jQuery('#probes-timeline-timedomaine').on('change', function() {
            probesTimeline.timeDomain(this.value);
        });
        jQuery('#probes-timeline-timedomaine').change();

        doVulnsDetailsHook();
        ptf_instrumentation.refresh();
        probesTimeline.update(context.probes.started, context.probes.ended);

        // Refresh data when task is not finished
        if (!isTaskFinished()) {
            var data = {
              authenticity_token: "jCF4thY3dTYEO9E/bjQO4LUA+0QWpQL63ZjYb0TnpZkWwXU2axfF9t7CAJPIeAE8rPXkdDdVn3Ao8YDqdH2jCA==",
              eta_progress_mod_speed: ''
            };

            var apiHTMLInterval = setInterval(function() {
              $$ = jQuery;
              $$.get('/sigtran/pta/tasks/1129?authenticity_token='+data['authenticity_token'], function(result) {
                var page = $$(result);
                var textareaScrollTop = document.getElementById("task_conf_detail").scrollTop;
                var to_refresh = ['#report_vulns', '#task-details', '#serv-schema', '#scan-controls']

                var div = document.createElement("div");
                div.innerHTML = result;
                div_eta_speed = div.querySelector('#eta_progress_mod_speed')
                if (div_eta_speed){
                    data.eta_progress_mod_speed = div.querySelector('#eta_progress_mod_speed').innerHTML
                }

                to_refresh.forEach(function(selector) {
                    $$(selector).html(page.find(selector).html());
                });

                document.getElementById("task_conf_detail").scrollTop = textareaScrollTop;
                    doVulnsDetailsHook();
                    ptf_instrumentation.refresh();
                });

                if(isTaskFinished()) {
                    clearInterval(apiHTMLInterval);
                }
            }, 5000);

            var apiJSONInterval = setInterval(function() {
                jQuery.get('/sigtran/pta/tasks/1129', function(result) {
                    var started_probes = JSON.parse(result.data.started).map(transformAPIResultToProbe);
                    var ended_probes = JSON.parse(result.data.ended).map(transformAPIResultToProbe);

                    context.probes.started = started_probes;
                    context.probes.ended = ended_probes;
                    probesTimeline.update(context.probes.started, context.probes.ended);

                    if(isTaskFinished()) {
                      clearInterval(apiJSONInterval);
                    }
                }, 'json');
            }, 5000);
        }
        else {
          probesTimeline.notifyTaskFinished(true);
        }

        function isTaskFinished() {
            return 'Finished' === document.getElementById('task-status').innerText
        }

        function doVulnsDetailsHook() {
            jQuery('.vuln-details').each(function(idx, elm) {
                var vulnBody = jQuery(elm).find('tbody');
                var vulnId = jQuery(elm).attr('vuln-id');
                var head = jQuery(elm).find('thead');

                head.attr('title', 'Click to show/hide its content');
                head.bind('click', function() {
                    if (!displayedVulns[vulnId]) {
                        displayedVulns[vulnId] = true;
                        vulnBody.show();
                    }
                    else {
                        displayedVulns[vulnId] = false;
                        vulnBody.hide();
                    }
                });

                if (!displayedVulns[vulnId]) {
                    vulnBody.hide();
                }
            });
        }


        // PTF Insturmentation
        function PtfInstrumentation() {
            // Iterarion replay slider
            var ITERATION_RANGE = 50;
            var start_iteration_input = document.getElementById('start-iteration');
            var stop_iteration_input = document.getElementById('stop-iteration');
            var slider = document.getElementById('slider-range');
            var default_range = {
                min: 0,
                max: 100
            };
            noUiSlider.create(slider,{
                start: [0, 50, 100], // [start_iteration, crash_iteration, stop_iteration]
                step: 1,
                connect: true,
                tooltips: true,
                range: default_range,
            });

            var origins = slider.getElementsByClassName('noUi-origin');
            Array.from(origins).forEach(function(o) { o.setAttribute('disabled', true); })
            // Fix zIndex
            origins[1].style.zIndex = 3;

            var select_tag = document.querySelectorAll('.ptf-instrumentation select')[0];
            select_tag.addEventListener('change', function() {
                var options = this.getElementsByTagName('option');
                for (var i=0; i<options.length; ++i) {
                    var option = options[i];
                    if (option.selected) {
                        setupSliderForEvent(
                            findEventByName(option.innerText),
                            slider,
                            default_range
                        );
                        break;
                    }
                }
            });

            this.refresh = function() {
                if (hasEvents()) {
                  enabledInstrumentationForm();
                  updateEventSelectOptions(select_tag, extractEventInfosFromDom());
                }
            };

            function findEventByName(event_name) {
                return extractEventInfosFromDom().filter(
                    function(item) {
                        return item.name == event_name;
                    })[0];
            }

            function setupSliderForEvent(item, slider, default_range) {
                var orgins = slider.getElementsByClassName('noUi-origin');
                origins[0].removeAttribute('disabled', true);
                origins[2].removeAttribute('disabled', true);

                default_range.min = (item.iteration - ITERATION_RANGE) >= 0 ? item.iteration - ITERATION_RANGE : 0;
                default_range.max = item.iteration + ITERATION_RANGE;

                slider.noUiSlider.updateOptions({
                        range: {
                            min: default_range.min,
                            max: default_range.max
                        },
                        start: [item.iteration, item.iteration, default_range.max]
                });
                document.getElementById('module-name').innerText = item.mod_name;
            }

            function hasEvents() {
                events = extractEventInfosFromDom();
                return events.length !== 0;
            }

            function extractEventInfosFromDom() {
                var event_infos = []
                var events = document.getElementsByClassName('vuln-details-head');

                for (var i=0; i<events.length; ++i) {
                    var e = events[i];
                    var table = e.parentElement;
                    var event_name = e.getElementsByTagName('th')[0].innerText.trim();
                    var event_raw_description = table.getElementsByClassName('technical-details')[0].innerText.trim();
                    var mod_name_match = /module name[^a-z]+ ([a-zA-Z_0-9]+)/gi.exec(event_raw_description);
                    var iteration_match = /iteration[^a-z]+ ([0-9]+)/gi.exec(event_raw_description);

                    if (!mod_name_match || !iteration_match) {
                        console.warn('No mod_name match | iteration match found in technical description');
                        continue;
                    }
                    event_infos.push({
                        name: event_name,
                        mod_name: mod_name_match[1],
                        iteration: parseInt(iteration_match[1]),
                        identifier: table.getAttribute('vuln-id')
                    });
                }
                return event_infos;
            }

            function updateEventSelectOptions(select_tag, event_infos) {
                var existing_event_names = {};
                var force_selection_update = !select_tag.value;

                for(var i=0; i<select_tag.children.length; ++i) {
                    var option = select_tag.children[i];
                    existing_event_names[option.innerText] = true;
                }

                event_infos.forEach(function(item) {
                    if (!existing_event_names[item.name]) {

                        var option = document.createElement('option');
                        option.value = item.mod_name;
                        option.innerText = item.name;
                        select_tag.add(option);

                    }
                });

                if (force_selection_update) {
                    var evt = document.createEvent('HTMLEvents');
                    evt.initEvent('change', false, true);
                    select_tag.dispatchEvent(evt);
                }
            }

            function enabledInstrumentationForm() {
                document.querySelectorAll('.ptf-instrumentation [disabled]')
                    .forEach(function(item) {
                        item.removeAttribute('disabled');
                   });
            }

            slider.noUiSlider.on('update', function(values, handle) {
                var value = parseInt(values[handle]),
                    referential_value = parseInt(values[1]);
                // Start handler
                if (handle === 0) {
                    if (value > referential_value) {
                        return slider.noUiSlider.set([referential_value, null, null]);
                    }
                    start_iteration_input.value = value;
                }
                // Stop handler
                else if (handle === 2) {
                    if (value === referential_value) {
                        return slider.noUiSlider.set([null, null, ++value]);
                    }
                    stop_iteration_input.value = value;
                }
            });

            start_iteration_input.addEventListener('change', function() {
                if (this.value < default_range.min) {
                    var new_start = slider.noUiSlider.get('start');
                    new_start[0] = parseInt(this.value) > 0 ? parseInt(this.value) : 0;

                    slider.noUiSlider.updateOptions({
                        range: {
                            min: new_start[0],
                            max: slider.noUiSlider.options.range.max
                        },
                        start: new_start
                    });
                }
                else if (this.value >= default_range.min && slider.noUiSlider.options.range.min < default_range.min) {
                    var new_start = slider.noUiSlider.get('start');
                    new_start[0] = parseInt(this.value);

                    slider.noUiSlider.updateOptions({
                        range: {
                            min: default_range.min,
                            max: slider.noUiSlider.options.range.max
                        },
                        start: new_start
                    });
                }
                else {
                    slider.noUiSlider.set([parseInt(this.value), null, null]);
                }
            });

            stop_iteration_input.addEventListener('change', function() {
                if (this.value > default_range.max) {
                    var new_start = slider.noUiSlider.get('start');
                    new_start[2] = parseInt(this.value);

                    slider.noUiSlider.updateOptions({
                        range: {
                            min: slider.noUiSlider.options.range.min,
                            max: parseInt(this.value)
                        },
                        start: new_start
                    });
                }
                else if (this.value <= default_range.max && slider.noUiSlider.options.range.max > default_range.max) {
                    var new_start = slider.noUiSlider.get('start');
                    new_start[2] = parseInt(this.value);

                    slider.noUiSlider.updateOptions({
                        range: {
                            min: slider.noUiSlider.options.range.min,
                            max: default_range.max
                        },
                        start: new_start
                    });
                }
                else {
                    slider.noUiSlider.set([null, null, parseInt(this.value)]);
                }
            });


            Array.from(document.getElementsByClassName('task-title')).forEach(function(elm) {
              elm.addEventListener('click', function(evt) {
                span_tag = elm.getElementsByTagName('span')[0]
                sibling_section_tag = elm.nextElementSibling;
                if (!span_tag || !sibling_section_tag || sibling_section_tag.tagName !== 'SECTION') { return; }
                    span_class = Array.from(span_tag.classList).join(',');
                    if (span_class.indexOf('glyphicon-menu-down') !== -1) {
                        span_tag.classList.remove('glyphicon-menu-down');
                        span_tag.classList.add('glyphicon-menu-right');
                        sibling_section_tag.style.display = 'none';
                  }
                  else {
                        span_tag.classList.remove('glyphicon-menu-right');
                        span_tag.classList.add('glyphicon-menu-down');
                        sibling_section_tag.style.display = 'block';
                  }
