    function transformAPIResultToProbe(elm) {
      var e = elm['pta_info_item'];
      e.data = JSON.parse(e.data);
      e.identifier = e.data.mod_name + e.data.iter_start + e.data.iter_stop + e.data.iter_stop_mod
      return e;
    }

    var context = {
      probes: {
        started : (
           function() {
           return [].map(transformAPIResultToProbe);
           })(),
        ended: (
          function() {
          return [].map(transformAPIResultToProbe);
          })()
      },
      task: (
