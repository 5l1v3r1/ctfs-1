      }
    ];

    /*
    Subscribe to the "beforerender" event, adding a submenu
    to each of the items in the MenuBar instance.
    */
    oMenuBar.subscribe("beforeRender", function () {
      if (this.getRoot() == this) {
        this.getItem(0).cfg.setProperty("submenu", aSubmenuData[0]);
        this.getItem(1).cfg.setProperty("submenu", aSubmenuData[1]);
        this.getItem(2).cfg.setProperty("submenu", aSubmenuData[2]);
        this.getItem(3).cfg.setProperty("submenu", aSubmenuData[3]);
        this.getItem(4).cfg.setProperty("submenu", aSubmenuData[4]);
        this.getItem(5).cfg.setProperty("submenu", aSubmenuData[5]);
        this.getItem(6).cfg.setProperty("submenu", aSubmenuData[6]);
        this.getItem(7).cfg.setProperty("submenu", aSubmenuData[7]);
        this.getItem(8).cfg.setProperty("submenu", aSubmenuData[8]);
      }
    });

    /*
    Call the "render" method with no arguments since the
    markup for this MenuBar instance is already exists in
    the page.
    */
    oMenuBar.render();
  };

  Event.onDOMReady(function() {
    var layout = new YAHOO.widget.Layout({
      units: [
      { position: 'top', height: 80, body: 'top1', scroll: null, zIndex: 2 },
          { position: 'right', header: 'Help', width: 250, resize: true, footer: 'Help', collapse: true, body: 'right1', animate: true, gutter: '5', scroll: true },
      { position: 'bottom', height: 30, body: 'bottom1' },
            { position: 'left', header: 'Access', width: 250, resize: true, footer: 'Access', collapse: true, body: 'left1', animate: true, gutter: '5', scroll: null, zIndex: 1 },
      { position: 'center', body: 'center1', scroll: true, gutter: '5 0' } // use gutter: '5 0' when having left and right layout panels, better display
      // { position: 'center', scroll: true, body: 'center1', gutter: '5' } // use gutter: '5 0' when having left and right layout panels, better display
      ]
    });

    layout.on('render', function() {
      YAHOO.util.Event.onContentReady("productsandservices", initTopMenu);
      // YAHOO.util.Event.onContentReady("productsandservices2", initLeftMenu);
    });

    layout.on('render', function() {

      // define helper function
      instantCollapse = function(panelName) {
        var p = layout.getUnitByPosition(panelName);
        p.set('animate', false);
        p.collapse();
        p.set('animate', true);
      };

      // define helper function
      setCookiePanel = function(cookieName, panelName) {
        var h = layout.getUnitByPosition(panelName);
        h.addListener("beforeCollapse", function() {
          YAHOO.util.Cookie.remove(cookieName);
          YAHOO.util.Cookie.set(cookieName, "true");
        } );
        h.addListener("beforeExpand", function() {
          YAHOO.util.Cookie.remove(cookieName);
          YAHOO.util.Cookie.set(cookieName, "false");
        } );
      };

      setCookiePanel("hide_help_panel", "right");
      setCookiePanel("hide_access_panel", "left");

      // default is to hide left panel and show right panel
      var hide_access_cookie = YAHOO.util.Cookie.get("hide_access_panel");
      if (!hide_access_cookie || (hide_access_cookie && hide_access_cookie === "true")) {
        instantCollapse('left');
      }
      var hide_help_cookie = YAHOO.util.Cookie.get("hide_help_panel");
      if (hide_help_cookie && hide_help_cookie === "true") {
        instantCollapse('right');
      }
    });

    layout.render();
    // layout.getUnitByPosition('right').toggle();
  });
})();
</script>

</body>
</html>
