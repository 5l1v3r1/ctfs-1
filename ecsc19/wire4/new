<script type="text/javascript">

    var interco_data = {"37":{"interconnection_ni":0,"interconnection_rc":0,"remote_ip":"127.0.0.1","remote_port":2905,"remote_pc":8889,"source_ip":"127.0.0.1","source_port":2906,"source_pc":8888,"source_gt":"123456789012542","target_pc_range":null,"target_gt_range":null,"target_msisdn":"123456789011111","target_msisdn_range":null,"target_imsi_range":"123456789011122"}}

    // Update interconnection fields when another license is selected
    document.getElementById("interconnection_id").onchange = function() {
        for (var k in interco_data) { // Get the list of field names
            for (var field in interco_data[k]) {
                if (document.getElementById(field)) {
                    document.getElementById(field).value = interco_data[this.value][field];
                }
            }
            break;
        }
    };

    // Fill fields with default params on page load
    document.getElementById("interconnection_id").onchange();

    jQuery(function ($) {

        /* BEGIN: SCAN MODE OPTN custom beahior */
        changeOptnView(jQuery('#scanmode-select').val()) // init

        jQuery('#scanmode-select').bind('change', function() {
            changeOptnView($(this).val());
        });

        function changeOptnView(scanmode) {
            resetOptnViews();
            jQuery('.'+scanmode+'-optn').show();
            if (scanmode === 'pta-ss7-high-level') {
                jQuery('form#new_task').attr('action', '/sigtran/pta/tasks/new_hl');
            }
            else {
                jQuery('form#new_task').attr('action', '/sigtran/pta/tasks/new_full');
            }
        }

        function resetOptnViews() {
            jQuery('#scanmode-select option').each(function(idx, elm) {
                jQuery('.'+elm.value+'-optn').hide();
            });
        }
        /* END: SCAN MODE OPTN custom beahior */

        // Form validation rules
        jQuery("#new_task")
            .validate({
                rules: {
                    "high_level_probes[]": {
                        required: true,
                        depends: function(element) {
                            return jQuery('#scanmode-select').val() === 'pta-ss7-high-level';
                        }
                    },
                    "target_imsi_range": "required",
                    "target_msisdn": "required",
                    "interconnection_ni": "required",
                    "interconnection_rc": "required",
                    "remote_ip": {
                        required: true,
                        ipv4: true
                    },
                    "remote_port": "required",
                    "remote_pc": "required",
                    "source_ip": {
                        required: true,
                        ipv4: true
                    },
                    "source_port": "required",
                    "source_pc": "required",
                    "source_gt": "required",
                },
                messages: {
                    "high_level_probes[]": "Please, select at least one probe type."
                }
        });

    // Form submit hook
    jQuery('#task_submit')
        .on('click', function() {

            // Set msisdn as default range if not defined
            var msisdn_input = jQuery('#target_msisdn'),
                msisdn_range_input = jQuery('#target_msisdn_range');

            if (!msisdn_range_input.val()) {
                msisdn_range_input.val(msisdn_input.val());
            }

            return true;
        });
    });

    // Use Subscriber MSISDN as default MSISDN range
    var msisdn_input = jQuery('#target_msisdn'),
        msisdn_range_input = jQuery('#target_msisdn_range');
    msisdn_range_input.attr('placeholder', msisdn_input.val());
    msisdn_input.bind('change', function() {
        msisdn_range_input.attr('placeholder', this.value);
    });

/* Scan mode high-level optn view multiselect */
    function setTitleAttributeForEach(titleValue, elems) {
        elems.each(function(idx, elm) {
            jQuery(elm).attr('title', titleValue);
        });
    }

    jQuery("#high-level-probes").multiSelect({
        selectableHeader: "<label class='multiselect-selectable-header'>Available</label><br /><input type='text' class='search-input' autocomplete='off' placeholder='Search ...'>",
        selectionHeader: "<label class='multiselect-selection-header'>Selected</label><br /><input type='text' class='search-input' autocomplete='off' placeholder='Search ...'>",
        afterInit: function(ms){
            msContext = ms;
            setTitleAttributeForEach('Click to select all group items', jQuery(msContext).find('.ms-selectable li.ms-optgroup-label'));
            setTitleAttributeForEach('Click to select', jQuery(msContext).find('li.ms-elem-selectable'));
            setTitleAttributeForEach('Click to deselect all group items', jQuery(msContext).find('.ms-selection li.ms-optgroup-label'));
            setTitleAttributeForEach('Click to deselect', jQuery(msContext).find('li.ms-elem.selection'));

            var that = this,
            $selectableSearch = that.$selectableUl.prev(),
            $selectionSearch = that.$selectionUl.prev(),
            selectableSearchString = '#'+that.$container.attr('id')+' .ms-elem-selectable:not(.ms-selected)',
            selectionSearchString = '#'+that.$container.attr('id')+' .ms-elem-selection.ms-selected';

            that.qs1 = $selectableSearch.quicksearch(selectableSearchString)
                .on('keydown', function(e){
                    if (e.which === 40){
                        that.$selectableUl.focus();
                        return false;
                    }
            });
            that.qs2 = $selectionSearch.quicksearch(selectionSearchString)
                .on('keydown', function(e){
                    if (e.which == 40){
                        that.$selectionUl.focus();
                        return false;
                    }
            });
        },
        afterSelect: function(selectedElms){
            this.qs1.cache();
            this.qs2.cache();
            setTitleAttributeForEach('Click to deselect all group items', jQuery(msContext).find('.ms-selection li.ms-optgroup-label'));
            setTitleAttributeForEach('Click to deselect', jQuery(msContext).find('li.ms-elem-selection'));
        },
        afterDeselect: function(removedElms){
            this.qs1.cache();
            this.qs2.cache();
            setTitleAttributeForEach('Click to select all group items', jQuery(msContext).find('.ms-selectable li.ms-optgroup-label'));
            setTitleAttributeForEach('Click to select', jQuery(msContext).find('li.ms-elem-selectable'));
        },
        selectableOptgroup: true
    });
</script>

	</section>
