    },
    {
      id: "support",
      itemdata: [
            { text: "Network configurations", url: "/pta_tools/net_configuration/1129" },
            { text: "Network tools", url: "/pta_tools/net_tools/1129" },
        { text: "Send invitation", url: "/invitations/new" },
        { text: "Change password", url: "/passwords/change" },
        { text: "Last Logins", url: "/lastlogins" },
